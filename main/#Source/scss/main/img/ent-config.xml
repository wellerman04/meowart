	<controller function="KEY_OBJECTPLACER_SPEED_CYCLE" button1="201" button2="-1"/>
	<controller function="KEY_OBJECTPLACER_FREEZE_TIME" button1="202" button2="-1"/>
	<controller function="KEY_OBJECTPLACER_FREEZE_POSITION" button1="192" button2="-1"/>
</controller_keys>

